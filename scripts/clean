#!/bin/sh -e

if [ -d 'dist' ] ; then
    rm -r dist
fi
if [ -d 'site' ] ; then
    rm -r site
fi
if [ -d 'databasex.egg-info' ] ; then
    rm -r databasex.egg-info
fi
if [ -d '.mypy_cache' ] ; then
    rm -r .mypy_cache
fi
if [ -d '.pytest_cache' ] ; then
    rm -r .pytest_cache
fi

find databasex -type f -name "*.py[co]" -delete
find databasex -type d -name __pycache__ -delete
